diff -urN linux-2.6.17/include/asm/seccomp.h linux-2.6.17-STEP/include/asm/seccomp.h
--- linux-2.6.17/include/asm/seccomp.h	1970-01-01 12:00:00.000000000 +1200
+++ linux-2.6.17-STEP/include/asm/seccomp.h	2006-06-18 13:49:35.000000000 +1200
@@ -0,0 +1,16 @@
+#ifndef _ASM_SECCOMP_H
+
+#include <linux/thread_info.h>
+
+#ifdef TIF_32BIT
+#error "unexpected TIF_32BIT on i386"
+#endif
